{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      ],
      "@babel/preset-react",
      "next/babel"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
          "module-name-mapper",
          {
              "moduleNameMapper": {
                // This needs to be in place for testing, but removed for css to work
                //  "\\.scss$": "<rootDir>/helpers/style-mock.js"
              }
          }
      ]
    ]
}